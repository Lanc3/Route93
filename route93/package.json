{
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web"
    ]
  },
  "devDependencies": {
    "@redwoodjs/auth-dbauth-setup": "8.8.1",
    "@redwoodjs/core": "8.8.1",
    "@redwoodjs/project-config": "8.8.1",
    "prettier-plugin-tailwindcss": "^0.5.12"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "yarn rw dev",
    "build": "yarn rw build",
    "deploy": "yarn rw deploy vercel",
    "vercel-build": "yarn rw build",
    "migrate:deploy": "yarn rw prisma migrate deploy",
    "db:seed": "yarn rw prisma db seed",
    "postdeploy": "yarn migrate:deploy && yarn db:seed",
    "prepare-deploy": "node scripts/prepare-deploy.js",
    "health-check": "curl -f http://localhost:8910/api/health || exit 1"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "packageManager": "yarn@4.6.0",
  "dependencies": {
    "@stripe/react-stripe-js": "^3.9.2",
    "@stripe/stripe-js": "^7.9.0",
    "crypto-js": "^4.2.0",
    "stripe": "^18.4.0"
  }
}
