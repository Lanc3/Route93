{
  "buildCommand": "yarn rw build",
  "outputDirectory": "web/dist",
  "installCommand": "yarn install",
  "framework": null,
  "functions": {
    "api/dist/functions/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}