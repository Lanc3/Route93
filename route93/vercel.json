{
  "buildCommand": "yarn rw build && yarn rw prisma migrate deploy",
  "outputDirectory": "web/dist",
  "installCommand": "yarn install",
  "framework": null,
  "functions": {
    "api/dist/functions/**/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}