{
  "buildCommand": "yarn vercel-build",
  "outputDirectory": "web/dist",
  "installCommand": "yarn install --frozen-lockfile",
  "framework": null,
  "functions": {
    "api/dist/functions/*.js": {
      "runtime": "@vercel/node@18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api/.*).*)",
      "destination": "/index.html"
    }
  ]
}
